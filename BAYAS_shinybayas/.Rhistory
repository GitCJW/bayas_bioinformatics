#' Title
#'
#' @param inputId
#' @param numberBtn
#' @param btnNames
#' @param default
#' @param width
#'
#' @return
#' @export
#'
#' @examples
groupedButtons <- function(inputId, numberBtn, btnNames, default=NULL,
width=NULL) {
listBtn <- lapply(1:numberBtn, function(i){
tags$button(id = paste0(inputId,"-",i),
class = ifelse(!is.null(default) && default==i,"groupedButtons btn btn-default btn-primary","groupedButtons btn btn-default"),
type = "button",
as.character(btnNames[i]),
'data-index' = i,
style=paste0("width: ",width,";"))
})
tagList(
singleton(tags$head(tags$script(src = "js/GroupedButtons.js"))),
tags$div(id = inputId,
class="groupedButtons-parent",
'data-name' = inputId,
'data-clicked' = default,
tagList(listBtn)
)
)
}
